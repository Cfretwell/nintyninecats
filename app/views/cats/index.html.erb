
<h1>Look at all these Cats! </h1>

<ul>
  <% @cats.each do |cat| %>
    <li> <%= link_to cat.name, url_for(cat)  %></li>
        <ul>
            <li> color: <%= cat.color %> </li>
            <li> age: <%= cat.age %> </li>
            <li> desc: <%= cat.description %>  </li>
        </ul>
  <% end %>
</ul>

<a href="<%= new_cat_url %>">Create new cat!</a>
